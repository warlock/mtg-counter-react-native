{
  "expo": {
    "name": "Magic Counter",
    "description": "MTG Life and Poison Counter",
    "slug": "magic-counter",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#000000",
    "primaryColor": "#000000",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "gl.js.magiccounter",
      "supportsTablet": true
    },
    "android": {
      "package": "gl.js.magiccounter",
      "versionCode": 3,
      "permissions": []
    }
  }
}
